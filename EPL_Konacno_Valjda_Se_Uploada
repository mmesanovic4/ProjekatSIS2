            "20. Spearman: 0.985 | Model: SVC, Features: ['HomeTeam', 'AwayTeam', 'Referee', 'B365H', 'Year', 'Domacin -1 utakmica'], Accuracy: 0.4763, Spearman: 0.9850\n"
          ]
        }
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "### TESTIRANJE ACCURACY PRVI: ###"
      ],
      "metadata": {
        "id": "lFra9KOq84Wx"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "from sklearn.svm import SVC\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "df = epl6\n",
        "train_data = df[df['Season'] < '2018/2019']\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "best_combinations = [\n",
        "    (\"Logistic Regression\", ['B365A', 'Season', 'Domacin -5 utakmica']),\n",
        "    (\"Logistic Regression\", ['B365A', 'Domacin -4 utakmica', 'Gost -1 utakmica']),\n",
        "    (\"SVC\", ['AwayTeam', 'B365H', 'Gost -5 utakmica']),\n",
        "    (\"SVC\", ['AwayTeam', 'B365H', 'Forma Gosta Nova']),\n",
        "    (\"Logistic Regression\", ['B365H', 'B365A', 'Month']),\n",
        "    (\"Logistic Regression\", ['B365H', 'B365A', 'Novi u Ligi Gost']),\n",
        "    (\"SVC\", ['B365H', 'Novi u Ligi Gost', 'Forma Gosta Nova']),\n",
        "    (\"SVC\", ['B365H', 'Domacin -5 utakmica', 'Forma Gosta Nova']),\n",
        "    (\"SVC\", ['B365D', 'B365A', 'Domacin -5 utakmica']),\n",
        "    (\"Logistic Regression\", ['B365A', 'Year', 'Gost -1 utakmica']),\n",
        "    (\"Logistic Regression\", ['B365A', 'Novi u Ligi Gost', 'Gost -1 utakmica']),\n",
        "    (\"SVC\", ['B365A', 'Novi u Ligi Gost', 'Gost -1 utakmica']),\n",
        "    (\"Logistic Regression\", ['B365A', 'Forma Domacina Numericki', 'Gost -1 utakmica']),\n",
        "    (\"Logistic Regression\", ['B365A', 'Otezana Forma Domacina', 'Gost -1 utakmica']),\n",
        "    (\"SVC\", ['B365A', 'Domacin -1 utakmica', 'Gost -1 utakmica']),\n",
        "    (\"SVC\", ['B365A', 'Domacin -1 utakmica', 'Forma Gosta Nova']),\n",
        "    (\"SVC\", ['B365A', 'Gost -1 utakmica', 'Gost -2 utakmica']),\n",
        "    (\"SVC\", ['B365A', 'Gost -1 utakmica', 'Forma Gosta Nova']),\n",
        "    (\"SVC\", ['B365A', 'Gost -2 utakmica', 'Forma Domacina Nova']),\n",
        "    (\"SVC\", ['HomeTeam', 'B365D', 'B365A'])\n",
        "]\n",
        "\n",
        "best_accuracy = 0\n",
        "best_model = None\n",
        "best_features = None\n",
        "best_predictions = None\n",
        "\n",
        "all_tables = []\n",
        "\n",
        "for model_name, features in best_combinations:\n",
        "    target = 'FTR'\n",
        "\n",
        "    X_train = train_data[features]\n",
        "    y_train = train_data[target]\n",
        "    X_eval = eval_data[features]\n",
        "\n",
        "    X_train_encoded = pd.get_dummies(X_train)\n",
        "    X_eval_encoded = pd.get_dummies(X_eval)\n",
        "    X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "    X_train_part, X_val, y_train_part, y_val = train_test_split(X_train_encoded, y_train, test_size=0.2)\n",
        "\n",
        "    if model_name == \"Random Forest\":\n",
        "        model = RandomForestClassifier()\n",
        "    elif model_name == \"SVC\":\n",
        "        model = SVC()\n",
        "    else:\n",
        "        model = LogisticRegression()\n",
        "\n",
        "    model.fit(X_train_part, y_train_part)\n",
        "    predictions = model.predict(X_eval_encoded)\n",
        "    accuracy = accuracy_score(eval_data[target], predictions)\n",
        "\n",
        "\n",
        "\n",
        "    league_table = {}\n",
        "    for idx, row in eval_data.iterrows():\n",
        "        home_team = row['HomeTeam']\n",
        "        away_team = row['AwayTeam']\n",
        "        predicted_result = predictions[idx - eval_data.index[0]]\n",
        "\n",
        "        for team in [home_team, away_team]:\n",
        "            if team not in league_table:\n",
        "                league_table[team] = {'Bodovi': 0, 'Pobjede': 0, 'Remiji': 0, 'Porazi': 0}\n",
        "\n",
        "        if predicted_result == 'H':\n",
        "            league_table[home_team]['Bodovi'] += 3\n",
        "            league_table[home_team]['Pobjede'] += 1\n",
        "            league_table[away_team]['Porazi'] += 1\n",
        "        elif predicted_result == 'A':\n",
        "            league_table[away_team]['Bodovi'] += 3\n",
        "            league_table[away_team]['Pobjede'] += 1\n",
        "            league_table[home_team]['Porazi'] += 1\n",
        "        else:\n",
        "            league_table[home_team]['Bodovi'] += 1\n",
        "            league_table[away_team]['Bodovi'] += 1\n",
        "            league_table[home_team]['Remiji'] += 1\n",
        "            league_table[away_team]['Remiji'] += 1\n",
        "\n",
        "    league_table_df = pd.DataFrame.from_dict(league_table, orient='index').reset_index()\n",
        "    league_table_df.rename(columns={'index': 'Tim'}, inplace=True)\n",
        "    league_table_df = league_table_df.sort_values(by=['Bodovi', 'Pobjede'], ascending=[False, False]).reset_index(drop=True)\n",
        "    league_table_df.index += 1\n",
        "    all_tables.append(league_table_df)\n",
        "\n",
        "    if accuracy > best_accuracy:\n",
        "        best_accuracy = accuracy\n",
        "        best_model = model\n",
        "        best_features = features\n",
        "        best_predictions = predictions\n",
        "        best_table = league_table_df\n",
        "\n",
        "print(f\"Najbolji model: {best_model}\")\n",
        "print(f\"Najbolje feature kombinacije: {best_features}\")\n",
        "print(f\"Najbolja tačnost: {best_accuracy:.5f}\")\n",
        "\n",
        "print(\"Predikcije za najbolji slučaj:\")\n",
        "correct_predictions = 0\n",
        "for idx, row in eval_data.iterrows():\n",
        "    pred = best_predictions[idx - eval_data.index[0]]\n",
        "    actual = row[target]\n",
        "    if pred == actual:\n",
        "        correct_predictions += 1\n",
        "    print(f\"Utakmica: {row['HomeTeam']} - {row['AwayTeam']}, Datum: {row['Date']}, Predviđeni rezultat: {pred}, Stvarni rezultat: {actual}\")\n",
        "print(f\"Ukupan broj tačno predviđenih utakmica: {correct_predictions}\")\n",
        "\n",
        "\n",
        "average_table = pd.concat(all_tables).groupby('Tim').mean().sort_values(by=['Bodovi'], ascending=False)\n",
        "\n",
        "team_positions = {}\n",
        "\n",
        "for table in all_tables:\n",
        "    for index, row in table.iterrows():\n",
        "        team = row['Tim']\n",
        "        position = index\n",
        "        if team not in team_positions:\n",
        "            team_positions[team] = {'total_position': 0, 'count': 0}\n",
        "        team_positions[team]['total_position'] += position\n",
        "        team_positions[team]['count'] += 1\n",
        "\n",
        "average_positions = {team: data['total_position'] / data['count']\n",
        "                     for team, data in team_positions.items()}\n",
        "\n",
        "average_table = average_table.reset_index()\n",
        "average_table['Prosječna pozicija'] = average_table['Tim'].map(average_positions)\n",
        "average_table = average_table.sort_values(by=['Prosječna pozicija'], ascending=True)\n",
        "average_table.index = average_table.index + 1\n",
        "\n",
        "\n",
        "pd.set_option(\"display.max_rows\", None)\n",
        "pd.set_option(\"display.max_columns\", None)\n",
        "\n",
        "print(\"Najbolji model po tačnosti:\")\n",
        "print(f\"Model: {best_model}, Features: {best_features}, Accuracy: {best_accuracy:.5f}\")\n",
        "print(best_table)\n",
        "print(\"\\nProsječna ligaška tabela na osnovu svih simulacija:\")\n",
        "print(average_table)\n",
        "\n",
        "pd.set_option(\"display.max_rows\", 10)\n",
        "pd.set_option(\"display.max_columns\", 10)\n"
      ],
      "metadata": {
