      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "# Pretpostavljam da je predictions DataFrame koji sadrži predviđene vrijednosti za statističke parametre\n",
        "# epl4 je originalni dataset koji sadrži stvarni FTR\n",
        "\n",
        "# Spojimo predikcije sa stvarnim FTR\n",
        "epl4_eval = epl4[epl4['Season'] == '2018/2019'].reset_index(drop=True)\n",
        "\n",
        "\n",
        "\n",
        "predictions_df = pd.DataFrame(predictions, columns=['HTR_predvidjeno','Sutevi_predvidjeno', 'Sutevi_u_okvir_predvidjeno', 'Crveni_Kartoni_predvidjeno', 'Zuti_Kartoni_predvidjeno'])\n",
        "data = pd.concat([predictions_df, epl4_eval[['FTR']]], axis=1)\n",
        "\n",
        "# Definišemo featurese i target za drugi model\n",
        "features_ftr = ['HTR_predvidjeno','Sutevi_predvidjeno', 'Sutevi_u_okvir_predvidjeno', 'Crveni_Kartoni_predvidjeno', 'Zuti_Kartoni_predvidjeno']\n",
        "target_ftr = 'FTR'\n",
        "\n",
        "# Maknemo redove koji nemaju vrijednost za FTR\n",
        "data = data.dropna(subset=[target_ftr])\n",
        "\n",
        "\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "# Podjela podataka na trening i test set\n",
        "X_train, X_test, y_train, y_test = train_test_split(data[features_ftr], data[target_ftr], test_size=0.2, random_state=42)\n",
        "\n",
        "# Inicijalizacija modela\n",
        "clf = RandomForestClassifier(n_estimators=100, random_state=42)\n",
        "\n",
        "# Trening modela\n",
        "clf.fit(X_train, y_train)\n",
        "\n",
        "# Testiranje modela\n",
        "y_pred = clf.predict(X_test)\n",
        "\n",
        "# Ocjena tačnosti\n",
        "accuracy = accuracy_score(y_test, y_pred)\n",
        "print(f'Točnost modela za predikciju rezultata: {accuracy:.4f}')\n",
        "\n",
        "\n",
        "# Predviđanje ishoda utakmice\n",
        "epl4_eval['Predicted_FTR'] = clf.predict(predictions_df[features_ftr])\n",
        "\n",
        "# Prikaz rezultata\n",
        "print(epl4_eval[['HomeTeam', 'AwayTeam', 'Predicted_FTR','FTR']].head())\n"
      ],
