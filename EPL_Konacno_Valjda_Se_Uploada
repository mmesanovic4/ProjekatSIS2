    {
      "cell_type": "code",
      "source": [
        "#Sad isto samo za otezanu formu\n",
        "epl4['Otezana Forma Domacina'] = 0\n",
        "epl4['Otezana Forma Gosta'] = 0\n",
        "\n",
        "def otezani(form):\n",
        "    faktor_tezine = [1, 2, 3, 4, 5] #Ovo su koeficijenti otezanja utakmica\n",
        "    points = []\n",
        "\n",
        "    for result in form:\n",
        "        if result == 'W':\n",
        "            points.append(3)\n",
        "        elif result == 'D':\n",
        "            points.append(1)\n",
        "        else:\n",
        "            points.append(0)\n",
        "\n",
        "    while len(points) < 5:\n",
        "        points.insert(0, 0)\n",
        "\n",
        "    return sum(p * w for p, w in zip(points[-5:], faktor_tezine)) #Otezana suma\n",
        "\n",
        "epl4['Otezana Forma Domacina'] = epl4['Forma Domacina'].apply(otezani)\n",
        "epl4['Otezana Forma Gosta'] = epl4['Forma Gosta'].apply(otezani)\n",
        "\n",
        "print(epl4.head())"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "eQBltadKwMJd",
        "outputId": "e99dccf6-2dda-4023-8987-02f98d93841f"
