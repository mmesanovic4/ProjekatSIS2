            "7      Southampton      67    22      1      15  45  65 -20\n",
            "8          Everton      64    21      1      16  54  46   8\n",
            "9   Crystal Palace      53    17      2      19  51  53  -2\n",
            "10     Bournemouth      52    17      1      20  56  70 -14\n",
            "11       Newcastle      50    16      2      20  42  48  -6\n",
            "12        West Ham      47    15      2      21  52  55  -3\n",
            "13          Fulham      43    14      1      23  34  81 -47\n",
            "14       Leicester      42    14      0      24  51  48   3\n",
            "15         Watford      35    11      2      25  52  59  -7\n",
            "16         Burnley      33    11      0      27  45  68 -23\n",
            "17    Huddersfield      33    11      0      27  22  76 -54\n",
            "18          Wolves      27     9      0      29  47  46   1\n",
            "19         Cardiff      17     5      2      31  34  69 -35\n",
            "20        Brighton      16     5      1      32  35  60 -25\n"
          ]
        }
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "### NAJNOVIJI POKUSAJ GDJE FORMA JE 0 NA POCETKU SEZONE ###\n"
      ],
      "metadata": {
        "id": "QBVlpLts217w"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Dodajemo nove kolone za formu domaćina i gosta\n",
        "\n",
        "epl5 = epl4\n",
        "epl5['Forma Domacina Nova'] = \"\"\n",
        "epl5['Forma Gosta Nova'] = \"\"\n",
        "\n",
        "# Grupisanje po sezoni da osiguramo reset forme na početku svake sezone\n",
        "seasons = epl5['Season'].unique()\n",
        "\n",
        "for season in seasons:\n",
        "    season_matches = epl5[epl5['Season'] == season]\n",
        "\n",
        "    # Lista svih timova u sezoni\n",
        "    teams = season_matches['HomeTeam'].unique()\n",
        "\n",
        "    # Praćenje forme po timu\n",
        "    team_form = {team: [] for team in teams}  # Reset forme za svaki tim na početku sezone\n",
        "\n",
        "    # Iteracija kroz utakmice te sezone\n",
        "    for i, row in season_matches.iterrows():\n",
        "        home_team = row['HomeTeam']\n",
        "        away_team = row['AwayTeam']\n",
        "\n",
        "        # Dohvaćanje forme do trenutne utakmice (maks 5 prethodnih utakmica)\n",
        "        home_results = \"\".join(team_form[home_team][-5:])\n",
        "        away_results = \"\".join(team_form[away_team][-5:])\n",
        "\n",
        "        # Postavljanje forme u dataset\n",
        "        epl5.at[i, 'Forma Domacina Nova'] = home_results\n",
        "        epl5.at[i, 'Forma Gosta Nova'] = away_results\n",
        "\n",
        "        # Ažuriranje forme nakon utakmice\n",
        "        if row['FTR'] == \"H\":\n",
        "            team_form[home_team].append(\"W\")\n",
        "            team_form[away_team].append(\"L\")\n",
        "        elif row['FTR'] == \"A\":\n",
        "            team_form[home_team].append(\"L\")\n",
        "            team_form[away_team].append(\"W\")\n",
        "        else:\n",
        "            team_form[home_team].append(\"D\")\n",
        "            team_form[away_team].append(\"D\")\n"
      ],
      "metadata": {
        "id": "6hGcCDec-KJQ"
      },
      "execution_count": 120,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "team_name = \"Liverpool\"\n",
        "\n",
        "# Filtriramo sve utakmice u kojima je Liverpool bio domaćin ili gost\n",
        "liverpool_matches = epl5[(epl5['HomeTeam'] == team_name) | (epl5['AwayTeam'] == team_name)].copy()\n",
        "\n",
        "# Kreiramo novu kolonu 'Forma Liverpoola' na osnovu toga da li je domaćin ili gost\n",
        "liverpool_matches['Forma Liverpoola'] = liverpool_matches.apply(\n",
        "    lambda row: row['Forma Domacina Nova'] if row['HomeTeam'] == team_name else row['Forma Gosta Nova'], axis=1\n",
        ")\n",
        "\n",
        "# Prikazujemo relevantne kolone\n",
        "pd.set_option(\"display.max_rows\", None)  # Resetovanje prikaza redova\n",
        "pd.set_option(\"display.max_columns\", None)  # Resetovanje prikaza kolona\n",
        "print(liverpool_matches[['Date', 'HomeTeam', 'AwayTeam', 'FTR', 'Forma Liverpoola']])\n",
        "pd.set_option(\"display.max_rows\", 10)  # Resetovanje prikaza redova\n",
