        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "epl2['Year'] = epl2['Date'].dt.year\n",
        "epl2['Month'] = epl2['Date'].dt.month\n",
        "\n",
        "def daj_sezonu(row):\n",
        "    year = row['Year']\n",
        "    month = row['Month']\n",
        "\n",
        "    if month >= 8:\n",
        "        return f\"{year}/{year + 1}\"\n",
        "    else:\n",
        "        return f\"{year - 1}/{year}\"\n",
        "\n",
        "epl2['Season'] = epl2.apply(daj_sezonu, axis=1)\n",
        "epl2.head()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
