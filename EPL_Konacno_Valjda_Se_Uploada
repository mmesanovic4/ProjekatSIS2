        "df = epl4\n",
        "train_data = df[df['Season'] < '2018/2019']\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "\n",
        "features = ['B365H', 'B365D', 'B365A','Novi u Ligi Domaci', 'Novi u Ligi Gost']\n",
        "target = 'FTR'\n",
        "\n",
        "X_train = train_data[features]\n",
        "y_train = train_data[target]\n",
        "X_train_encoded = pd.get_dummies(X_train)\n",
        "X_train_part, X_val, y_train_part, y_val = train_test_split(X_train_encoded, y_train, test_size=0.2, random_state=42)\n",
        "\n",
        "\n",
        "model = RandomForestClassifier(random_state=42)\n",
        "model.fit(X_train_part, y_train_part)"
