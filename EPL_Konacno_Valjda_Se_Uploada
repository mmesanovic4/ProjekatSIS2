    {
      "cell_type": "code",
      "source": [
        "#Dodavanje novoligasa i forme u dataset\n",
        "epl3['Novi u Ligi Domaci'] = 0\n",
        "epl3['Novi u Ligi Gost'] = 0\n",
        "\n",
        "promoted_teams = {\n",
        "    \"2010/2011\": [\"Newcastle\", \"Blackpool\", \"West Brom\"],\n",
        "}\n",
        "\n",
        "seasons = sorted(epl3['Season'].unique())\n",
        "for season in seasons:\n",
        "    if season in promoted_teams:\n",
        "        #3 nove ekipe\n",
        "        epl3.loc[(epl3['Season'] == season) & epl3['HomeTeam'].isin(promoted_teams[season]), 'Novi u Ligi Domaci'] = 1\n",
        "        epl3.loc[(epl3['Season'] == season) & epl3['AwayTeam'].isin(promoted_teams[season]), 'Novi u Ligi Gost'] = 1\n",
        "    else:\n",
        "        prosla_sezona_index = seasons.index(season) - 1\n",
        "        if prosla_sezona_index >= 0:\n",
        "            prosla_sezona = seasons[prosla_sezona_index]\n",
        "            previous_teams = set(epl3[epl3['Season'] == prosla_sezona]['HomeTeam']).union(\n",
        "                set(epl3[epl3['Season'] == prosla_sezona]['AwayTeam']))\n",
        "\n",
        "            #Ako tim nije bio u prethodnoj sezoni vrijednost je 1\n",
        "            epl3.loc[(epl3['Season'] == season) & ~epl3['HomeTeam'].isin(previous_teams), 'Novi u Ligi Domaci'] = 1\n",
        "            epl3.loc[(epl3['Season'] == season) & ~epl3['AwayTeam'].isin(previous_teams), 'Novi u Ligi Gost'] = 1\n",
        "\n",
        "epl3['Forma Domacina'] = \"\"\n",
        "epl3['Forma Gosta'] = \"\"\n",
        "\n",
        "teams = epl3['HomeTeam'].unique()\n",
        "\n",
        "for i, row in epl3.iterrows():\n",
        "    home_team = row['HomeTeam']\n",
        "    away_team = row['AwayTeam']\n",
        "\n",
        "    tekme_kuca = epl3[(epl3['HomeTeam'] == home_team) | (epl3['AwayTeam'] == home_team)]\n",
        "    tekme_kuca = tekme_kuca[tekme_kuca['Date'] < row['Date']].sort_values(by='Date', ascending=False)\n",
        "\n",
        "    tekme_gost = epl3[(epl3['HomeTeam'] == away_team) | (epl3['AwayTeam'] == away_team)]\n",
        "    tekme_gost = tekme_gost[tekme_gost['Date'] < row['Date']].sort_values(by='Date', ascending=False)\n",
        "\n",
        "    rez_kuca = []\n",
        "    rez_gost = []\n",
        "\n",
        "    #Domaćin\n",
        "    for _, home_row in tekme_kuca.iterrows():\n",
        "        if home_row['HomeTeam'] == home_team:\n",
        "            rez_kuca.append(\"W\" if home_row['FTR'] == \"H\" else \"D\" if home_row['FTR'] == \"D\" else \"L\")\n",
        "        elif home_row['AwayTeam'] == home_team:\n",
        "            rez_kuca.append(\"W\" if home_row['FTR'] == \"A\" else \"D\" if home_row['FTR'] == \"D\" else \"L\")\n",
        "        if len(rez_kuca) >= 5:  #5 tekmi\n",
        "            break\n",
        "    epl3.at[i, 'Forma Domacina'] = \"\".join(reversed(rez_kuca))\n",
        "\n",
        "    #Gost\n",
        "    for _, away_row in tekme_gost.iterrows():\n",
        "        if away_row['HomeTeam'] == away_team:\n",
        "            rez_gost.append(\"W\" if away_row['FTR'] == \"H\" else \"D\" if away_row['FTR'] == \"D\" else \"L\")\n",
        "        elif away_row['AwayTeam'] == away_team:\n",
        "            rez_gost.append(\"W\" if away_row['FTR'] == \"A\" else \"D\" if away_row['FTR'] == \"D\" else \"L\")\n",
        "        if len(rez_gost) >= 5:  #5 tekmi\n",
        "            break\n",
        "    epl3.at[i, 'Forma Gosta'] = \"\".join(reversed(rez_gost))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
