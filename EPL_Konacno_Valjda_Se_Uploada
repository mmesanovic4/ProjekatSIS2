    {
      "cell_type": "code",
      "source": [
        "df = epl4\n",
        "train_data = df[df['Season'] < '2018/2019']\n",
        "# Evaluacija: sezona 2018/19\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "# Odabir relevantnih kolona\n",
        "\n",
        "\n",
        "features = ['HomeTeam', 'AwayTeam','HTR', 'Poredjenje_Suteva', 'Poredjenje_Suteva_u_Okvir', 'Poredjenje_Crvenih_Kartona', 'Poredjenje_Zutih_Kartona']\n",
        "target = 'FTR'  # pretpostavljamo da je FTR ciljna varijabla (npr. H/D/A)\n",
        "\n",
        "# Priprema skupa za trening\n",
        "X_train = train_data[features]\n",
        "y_train = train_data[target]\n",
        "\n",
        "# Ako su feature vrijednosti kategorijske, primijenite one-hot encoding:\n",
        "X_train_encoded = pd.get_dummies(X_train)\n",
        "\n",
        "# Podjela skupa treninga na trening i test (validacijski skup) unutar pre-2017/18\n",
        "X_train_part, X_val, y_train_part, y_val = train_test_split(X_train_encoded, y_train, test_size=0.2)\n",
        "\n",
        "# Trening modela (npr. Random Forest)\n",
        "model = RandomForestClassifier()\n",
        "model.fit(X_train_part, y_train_part)"
      ],
