    {
      "cell_type": "code",
      "source": [
        "#Sad svaka posebno tekma\n",
        "epl4[\"Domacin -1 utakmica\"] = 0\n",
        "epl4[\"Domacin -2 utakmica\"] = 0\n",
        "epl4[\"Domacin -3 utakmica\"] = 0\n",
        "epl4[\"Domacin -4 utakmica\"] = 0\n",
        "epl4[\"Domacin -5 utakmica\"] = 0\n",
        "\n",
        "epl4[\"Gost -1 utakmica\"] = 0\n",
        "epl4[\"Gost -2 utakmica\"] = 0\n",
        "epl4[\"Gost -3 utakmica\"] = 0\n",
        "epl4[\"Gost -4 utakmica\"] = 0\n",
        "epl4[\"Gost -5 utakmica\"] = 0\n",
        "\n",
        "def poenii1(form):\n",
        "    points = []\n",
        "    for result in form:\n",
        "        if result == 'W':\n",
        "            points.append('W')\n",
        "        elif result == 'D':\n",
        "            points.append('D')\n",
        "        else:\n",
        "            points.append('L')\n",
        "    while len(points) < 5:  #Ako fali tekmi, popunjava se sa 0\n",
        "        points.insert(0, None)\n",
        "    return points[-5:]\n",
        "\n",
        "domacin = epl4['Forma Domacina'].apply(poenii1).apply(pd.Series)\n",
        "gost = epl4['Forma Gosta'].apply(poenii1).apply(pd.Series)\n",
        "domacin.columns = [\"Domacin -5 utakmica\", \"Domacin -4 utakmica\", \"Domacin -3 utakmica\", \"Domacin -2 utakmica\", \"Domacin -1 utakmica\"]\n",
        "gost.columns = [\"Gost -5 utakmica\", \"Gost -4 utakmica\", \"Gost -3 utakmica\", \"Gost -2 utakmica\", \"Gost -1 utakmica\"]\n",
        "\n",
        "epl4 = pd.concat([epl4, domacin, gost], axis=1)\n",
        "\n",
        "print(epl4.head())"
      ],
      "metadata": {
