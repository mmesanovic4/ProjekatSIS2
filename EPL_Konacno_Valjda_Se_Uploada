          "metadata": {},
          "execution_count": 131
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "epl6['Date'] = pd.to_datetime(epl6['Date'])\n",
        "\n",
        "#Inicijaliziramo nove kolone\n",
        "epl6['Dani od zadnjeg meca'] = 0\n",
        "epl6['Dani od zadnjeg meca reset'] = 0\n",
        "\n",
        "teams = set(epl6['HomeTeam'].unique()) | set(epl6['AwayTeam'].unique())\n",
        "\n",
        "for team in teams:\n",
        "    team_matches = epl6[(epl6['HomeTeam'] == team) | (epl6['AwayTeam'] == team)].sort_values(by='Date')\n",
        "\n",
        "    prev_date = None\n",
        "    prev_season = None\n",
        "\n",
        "    for i, row in team_matches.iterrows():\n",
        "        current_date = row['Date']\n",
        "        current_season = row['Season']\n",
        "\n",
        "        if prev_date is not None:\n",
        "            days_diff = (current_date - prev_date).days\n",
        "\n",
        "            #Ako je nova sezona, broj dana za slucaj reset je 0\n",
        "            if current_season != prev_season:\n",
        "                epl6.at[i, 'Dani od zadnjeg meca'] = days_diff\n",
        "                epl6.at[i, 'Dani od zadnjeg meca reset'] = 0\n",
        "            else:\n",
        "                epl6.at[i, 'Dani od zadnjeg meca'] = days_diff\n",
        "                epl6.at[i, 'Dani od zadnjeg meca reset'] = days_diff\n",
        "        else:\n",
        "            #Prva razmatrana utakmica\n",
        "            epl6.at[i, 'Dani od zadnjeg meca'] = 0\n",
        "            epl6.at[i, 'Dani od zadnjeg meca reset'] = 0\n",
        "\n",
        "        prev_date = current_date\n",
        "        prev_season = current_season\n"
      ],
      "metadata": {
        "id": "LbjQ6IPNV2RB"
      },
      "execution_count": 132,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "liverpool_matches = epl6[(epl6['HomeTeam'] == 'Liverpool') | (epl6['AwayTeam'] == 'Liverpool')]\n",
        "liverpool_matches = liverpool_matches[['Date', 'HomeTeam', 'AwayTeam', 'Dani od zadnjeg meca', 'Dani od zadnjeg meca reset']]\n",
        "liverpool_matches = liverpool_matches.sort_values(by='Date')\n",
        "\n",
        "pd.set_option(\"display.max_rows\", None)\n",
        "pd.set_option(\"display.max_columns\", None)\n",
        "print(liverpool_matches)\n",
        "pd.set_option(\"display.max_rows\", 10)\n",
        "pd.set_option(\"display.max_columns\", 10)"
