        "import pandas as pd\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "# Učitavanje dataset-a\n",
        "df = epl4\n",
        "\n",
        "# Filtriranje podataka za sezonu 2018/19\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "# Odabir relevantnih kolona\n",
        "features = ['HomeTeam', 'AwayTeam','Otezana Forma Domacina', 'Otezana Forma Gosta']\n",
        "target = ['HTR_Int','Poredjenje_Suteva', 'Poredjenje_Suteva_u_Okvir','Poredjenje_Crvenih_Kartona','Poredjenje_Zutih_Kartona']  # pretpostavljamo da je FTR ciljna varijabla (npr. H/D/A)\n",
        "\n",
        "# Priprema podataka za evaluaciju\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "\n",
        "# Uskladite stupce s onima koji su korišteni pri treningu\n",
        "# (pretpostavljamo da ste već kreirali X_train_encoded iz skupa podataka prije 2017/18)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "# Predviđanje (pretpostavljamo da je model već istreniran)\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "\n",
        "import pandas as pd\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "# Učitavanje dataset-a\n",
        "df = epl4\n",
        "\n",
        "# Filtriranje podataka za sezonu 2018/19\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "# Odabir relevantnih kolona\n",
        "features = ['HomeTeam', 'AwayTeam','Otezana Forma Domacina', 'Otezana Forma Gosta']\n",
        "target = ['HTR_Int','Poredjenje_Suteva', 'Poredjenje_Suteva_u_Okvir','Poredjenje_Crvenih_Kartona','Poredjenje_Zutih_Kartona']  # pretpostavljamo da je FTR ciljna varijabla (npr. H/D/A)\n",
        "\n",
        "# Priprema podataka za evaluaciju\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "\n",
        "# Uskladite stupce s onima koji su korišteni pri treningu\n",
        "# (pretpostavljamo da ste već kreirali X_train_encoded iz skupa podataka prije 2017/18)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "# Predviđanje (pretpostavljamo da je model već istreniran)\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "\n"
