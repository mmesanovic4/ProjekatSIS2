    {
      "cell_type": "code",
      "source": [
        "df = epl3\n",
        "\n",
        "eval_data = df[df['Season'] == '2018/2019'].copy()\n",
        "eval_data['Date'] = pd.to_datetime(eval_data['Date'])\n",
        "eval_data.sort_values(by='Date', inplace=True)\n",
        "eval_data['Date_str'] = eval_data['Date'].dt.strftime('%d-%m-%Y')\n",
        "\n",
        "target = 'FTR'\n",
        "\n",
        "#Postavljamo veličinu prozora za formu – 5 utakmica\n",
        "prozor = 5\n",
        "\n",
        "#Promjenljiva forma\n",
        "forma_kuca = {}\n",
        "forma_gost = {}\n",
        "\n",
        "#Tabela\n",
        "league_table = {}\n",
        "match_info = []\n",
        "predictions_list = []\n",
        "actual_list = []\n",
        "\n",
        "for pos, (idx, row) in enumerate(eval_data.iterrows()):\n",
        "    home_team = row['HomeTeam']\n",
        "    away_team = row['AwayTeam']\n",
        "\n",
        "    if home_team not in forma_kuca:\n",
        "        forma_kuca[home_team] = row['Forma Domacina']\n",
        "    if away_team not in forma_gost:\n",
        "        forma_gost[away_team] = row['Forma Gosta']\n",
        "\n",
        "    feat_dict = {\n",
        "        'HomeTeam': home_team,\n",
        "        'AwayTeam': away_team,\n",
        "        'Referee': row['Referee'],\n",
        "        'Forma Domacina': forma_kuca[home_team],\n",
        "        'Forma Gosta': forma_gost[away_team]\n",
        "    }\n",
        "\n",
        "    #Pretvaramo u DataFrame i radimo one-hot enkodiranje\n",
        "    feat_df = pd.DataFrame([feat_dict])\n",
        "    feat_df_encoded = pd.get_dummies(feat_df)\n",
        "    feat_df_encoded = feat_df_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "    pred = model.predict(feat_df_encoded)[0]\n",
        "    predictions_list.append(pred)\n",
        "    actual = row[target]\n",
        "    actual_list.append(actual)\n",
        "\n",
        "    match_info.append({\n",
        "        'Utakmica': f\"{home_team} - {away_team}\",\n",
        "        'Datum': row['Date_str'],\n",
        "        'Forma Domacina': forma_kuca[home_team],\n",
        "        'Forma Gosta': forma_gost[away_team],\n",
        "        'Predvidjeni rezultat': pred,\n",
        "        'Stvarni rezultat': actual\n",
        "    })\n",
        "\n",
        "    for team in [home_team, away_team]:\n",
        "        if team not in league_table:\n",
        "            league_table[team] = {'Bodovi': 0, 'Pobjede': 0, 'Remiji': 0, 'Porazi': 0}\n",
        "\n",
        "    if pred == 'H':\n",
        "        league_table[home_team]['Bodovi'] += 3\n",
        "        league_table[home_team]['Pobjede'] += 1\n",
        "        league_table[away_team]['Porazi'] += 1\n",
        "        new_home_outcome = 'W'\n",
        "        new_away_outcome = 'L'\n",
        "    elif pred == 'A':\n",
        "        league_table[away_team]['Bodovi'] += 3\n",
        "        league_table[away_team]['Pobjede'] += 1\n",
        "        league_table[home_team]['Porazi'] += 1\n",
        "        new_home_outcome = 'L'\n",
        "        new_away_outcome = 'W'\n",
        "    else:\n",
        "        league_table[home_team]['Bodovi'] += 1\n",
        "        league_table[away_team]['Bodovi'] += 1\n",
        "        league_table[home_team]['Remiji'] += 1\n",
        "        league_table[away_team]['Remiji'] += 1\n",
        "        new_home_outcome = 'D'\n",
        "        new_away_outcome = 'D'\n",
        "\n",
        "    #Azuriranje forme za domacina i gosta\n",
        "    if len(forma_kuca[home_team]) >= prozor:\n",
        "        forma_kuca[home_team] = forma_kuca[home_team][1:] + new_home_outcome\n",
        "    else:\n",
        "        forma_kuca[home_team] += new_home_outcome\n",
        "\n",
        "    if len(forma_gost[away_team]) >= prozor:\n",
        "        forma_gost[away_team] = forma_gost[away_team][1:] + new_away_outcome\n",
        "    else:\n",
        "        forma_gost[away_team] += new_away_outcome\n",
        "\n",
        "tacnost = accuracy_score(actual_list, predictions_list)\n",
        "\n",
        "print(\"Rezultati za sezonu 2018/19:\")\n",
        "print(\"-\" * 60)\n",
        "for m in match_info:\n",
        "    print(f\"Utakmica: {m['Utakmica']}, Datum: {m['Datum']}, \"\n",
        "          f\"Forma Domacina: {m['Forma Domacina']}, Forma Gosta: {m['Forma Gosta']}, \"\n",
        "          f\"Predvidjeni rezultat: {m['Predvidjeni rezultat']}, Stvarni rezultat: {m['Stvarni rezultat']}\")\n",
        "\n",
        "print(\"-\" * 60)\n",
        "print(f\"Ukupna tačnost rezultata: {tacnost:.5f}\")"
      ],
