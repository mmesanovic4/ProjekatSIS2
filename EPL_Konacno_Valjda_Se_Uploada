    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "# Inicijalizacija DataFrame-a za tabelu lige\n",
        "teams = set(epl4_eval['HomeTeam'].unique()) | set(epl4_eval['AwayTeam'].unique())\n",
        "league_table = pd.DataFrame({'Team': list(teams), 'Points': 0, 'GF': 0, 'GA': 0, 'GD': 0, 'Wins': 0, 'Draws': 0, 'Losses': 0})\n",
        "\n",
        "# Iteracija kroz utakmice i dodavanje bodova\n",
        "for _, row in epl4_eval.iterrows():\n",
        "    home_team = row['HomeTeam']\n",
        "    away_team = row['AwayTeam']\n",
        "    result = row['Predicted_FTR']\n",
        "    home_goals = row['FTHG']  # Predviđeni golovi domaćina\n",
        "    away_goals = row['FTAG']  # Predviđeni golovi gosta\n",
        "\n",
        "    # Dodavanje golova\n",
        "    league_table.loc[league_table['Team'] == home_team, 'GF'] += home_goals\n",
        "    league_table.loc[league_table['Team'] == home_team, 'GA'] += away_goals\n",
        "    league_table.loc[league_table['Team'] == away_team, 'GF'] += away_goals\n",
        "    league_table.loc[league_table['Team'] == away_team, 'GA'] += home_goals\n",
        "\n",
        "    # Dodavanje bodova i broja pobjeda/remija/poraza\n",
        "    if result == 'H':  # Domaćin pobijedio\n",
        "        league_table.loc[league_table['Team'] == home_team, 'Points'] += 3\n",
        "        league_table.loc[league_table['Team'] == home_team, 'Wins'] += 1\n",
        "        league_table.loc[league_table['Team'] == away_team, 'Losses'] += 1\n",
        "    elif result == 'A':  # Gost pobijedio\n",
        "        league_table.loc[league_table['Team'] == away_team, 'Points'] += 3\n",
        "        league_table.loc[league_table['Team'] == away_team, 'Wins'] += 1\n",
        "        league_table.loc[league_table['Team'] == home_team, 'Losses'] += 1\n",
        "    elif result == 'D':  # Remi\n",
        "        league_table.loc[league_table['Team'] == home_team, 'Points'] += 1\n",
        "        league_table.loc[league_table['Team'] == away_team, 'Points'] += 1\n",
        "        league_table.loc[league_table['Team'] == home_team, 'Draws'] += 1\n",
        "        league_table.loc[league_table['Team'] == away_team, 'Draws'] += 1\n",
        "\n",
        "# Izračunavanje gol-razlike (GD = GF - GA)\n",
        "league_table['GD'] = league_table['GF'] - league_table['GA']\n",
        "\n",
        "# Sortiranje po bodovima i zatim po gol-razlici\n",
        "league_table = league_table.sort_values(by=['Points', 'GD', 'GF'], ascending=[False, False, False]).reset_index(drop=True)\n",
        "\n",
        "# Dodavanje brojeva redova u tabeli (pozicija na tabeli)\n",
        "league_table.index += 1\n",
        "\n",
        "# Ispis konačne tabele\n",
        "print(\"\\nLigaška tabela na osnovu predviđenih rezultata:\")\n",
        "pd.set_option(\"display.max_rows\", None)  # Prikazuje sve redove\n",
        "pd.set_option(\"display.max_columns\", None)  # Prikazuje sve kolone\n",
        "print(league_table[['Team', 'Points', 'Wins', 'Draws', 'Losses', 'GF', 'GA', 'GD']])\n",
        "pd.set_option(\"display.max_rows\", 10)  # Resetovanje prikaza redova\n",
        "pd.set_option(\"display.max_columns\", 10)  # Resetovanje prikaza kolona\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "13e3e6c3-6a14-4984-84b4-4f97e7c4c9a8",
        "id": "V88qYPshIghS"
      },
      "execution_count": 109,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
