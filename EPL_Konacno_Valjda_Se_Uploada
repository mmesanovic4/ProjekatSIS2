    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import LabelEncoder, StandardScaler\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "from sklearn.metrics import accuracy_score, classification_report"
      ],
      "metadata": {
        "id": "iA0rcDKMzutH"
      },
      "execution_count": 58,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df = epl3\n",
        "train_data = df[df['Season'] < '2018/2019']\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "#Ulazi\n",
        "features = ['HomeTeam', 'AwayTeam', 'Referee']\n",
        "target = 'FTR'  #Izlaz\n",
        "\n",
        "#Skup treninga\n",
        "X_train = train_data[features]\n",
        "y_train = train_data[target]\n",
        "\n",
        "#Za dobivanje numerickih vrijednosti\n",
        "X_train_encoded = pd.get_dummies(X_train)\n",
        "\n",
        "X_train_part, X_val, y_train_part, y_val = train_test_split(X_train_encoded, y_train, test_size=0.2, random_state=42)\n",
        "\n",
        "#Random forest algoritam\n",
        "model = RandomForestClassifier(random_state=42)\n",
        "model.fit(X_train_part, y_train_part)\n",
        "\n",
        "#Validacija\n",
        "accuracy = model.score(X_val, y_val)\n",
        "print(\"Validacijska tačnost:\", accuracy)\n",
        "\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "#Predviđanje\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "print(\"Predikcije za sezonu 2018/19:\", predictions)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
