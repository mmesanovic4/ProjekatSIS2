        "# Kreiranje i sortiranje ligaške tabele\n",
        "league_df = pd.DataFrame.from_dict(league_table, orient='index').reset_index().rename(columns={'index': 'Tim'})\n",
        "league_df = league_df.sort_values(by=['Bodovi', 'Pobjede'], ascending=[False, False]).reset_index(drop=True)\n",
        "league_df.index += 1  # Indeks počinje od 1\n",
        "print(\"\\nLigaška tabela na osnovu predviđenih rezultata:\")\n",
