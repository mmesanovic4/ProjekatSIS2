      "cell_type": "code",
      "source": [
        "epl10 = pd.read_csv(\"E0 2010-2011.csv\")\n",
        "epl11 = pd.read_csv(\"E0 2011-2012.csv\")\n",
        "epl12 = pd.read_csv(\"E0 2012-2013.csv\")\n",
        "epl13 = pd.read_csv(\"E0 2013-2014.csv\")\n",
        "epl14 = pd.read_csv(\"E0 2014-2015.csv\")\n",
        "epl15 = pd.read_csv(\"E0 2015-2016.csv\")\n",
        "epl16 = pd.read_csv(\"E0 2016-2017.csv\")\n",
        "epl17 = pd.read_csv(\"E0 2017-2018.csv\")\n",
        "epl18 = pd.read_csv(\"E0 2018-2019.csv\")\n",
        "\n",
        "dataframes = [epl10, epl11, epl12, epl13, epl14, epl15, epl16, epl17, epl18]\n",
        "\n",
        "# Pronađi zajedničke kolone\n",
        "kolone = list(epl10.columns)\n",
        "for df in dataframes[1:]:\n",
        "    kolone = [col for col in kolone if col in df.columns]\n",
        "\n",
        "if not kolone:\n",
        "    print(\"Nema zajedničkih kolona u svim datotekama.\")\n",
        "else:\n",
        "    novi_dataframe = pd.concat([df[kolone] for df in dataframes], ignore_index=True, axis=0)\n",
        "    novi_dataframe.to_csv(\"EPL_potpuni.csv\", index=False)\n",
        "\n",
        "epl = pd.read_csv(\"EPL_potpuni.csv\")\n",
        "epl.head()"
