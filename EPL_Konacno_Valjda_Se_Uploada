      "cell_type": "code",
      "source": [
        "epl4 = epl3.copy() #Novi dataset pravimo\n",
        "epl4['Forma Domacina Numericki'] = 0\n",
        "epl4['Forma Gosta Numericki'] = 0\n",
        "\n",
        "def poenii(form):\n",
        "    points = 0\n",
        "    for result in form:\n",
        "        if result == 'W':\n",
        "            points += 3\n",
        "        elif result == 'D':\n",
        "            points += 1\n",
        "        else: points += 0\n",
        "    return points\n",
        "\n",
        "#Prelaz iz stringa u numericki rezultat\n",
        "epl4['Forma Domacina Numericki'] = epl4['Forma Domacina'].apply(poenii)\n",
        "epl4['Forma Gosta Numericki'] = epl4['Forma Gosta'].apply(poenii)\n",
        "\n",
        "print(epl4.head())"
      ],
      "metadata": {
