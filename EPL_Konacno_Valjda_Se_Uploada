    {
      "cell_type": "code",
      "source": [
        "# Play an audio beep. Any audio URL will do.\n",
        "from google.colab import output\n",
        "output.eval_js('new Audio(\"https://upload.wikimedia.org/wikipedia/commons/0/05/Beep-09.ogg\").play()')"
      ],
      "metadata": {
        "id": "vH_YU-l5fu0E"
      },
      "execution_count": 142,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import itertools\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.ensemble import RandomForestClassifier, GradientBoostingClassifier\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.svm import SVC\n",
        "from sklearn.neighbors import KNeighborsClassifier\n",
        "from sklearn.metrics import accuracy_score\n",
        "from sklearn.model_selection import train_test_split\n",
        "from scipy.stats import spearmanr\n",
        "\n",
        "#Sve moguce opcije ulaza\n",
        "moguci_ulazi = [\n",
        "    'HomeTeam', 'AwayTeam', 'Referee', 'B365H', 'B365D', 'B365A', 'Year',\n",
        "    'Month', 'Season','Novi u Ligi Domaci', 'Novi u Ligi Gost',\n",
        "    'Forma Domacina Numericki', 'Forma Gosta Numericki',\n",
        "    'Otezana Forma Domacina', 'Otezana Forma Gosta',\n",
        "    'Domacin -1 utakmica', 'Domacin -2 utakmica', 'Domacin -3 utakmica',\n",
        "    'Domacin -4 utakmica', 'Domacin -5 utakmica',\n",
        "    'Gost -1 utakmica', 'Gost -2 utakmica', 'Gost -3 utakmica',\n",
        "    'Gost -4 utakmica', 'Gost -5 utakmica',\n",
        "    'Forma Domacina Nova', 'Forma Gosta Nova',\n",
        "    'Dani od zadnjeg meca', 'Dani od zadnjeg meca reset'\n",
        "]\n",
        "\n",
        "target = 'FTR'\n",
        "\n",
        "train_data = df[df['Season'] < '2018/2019'].copy()\n",
        "eval_data = df[df['Season'] == '2018/2019'].copy()\n",
        "\n",
        "#Razliciti koristeni modeli\n",
        "models = {\n",
        "    \"Random Forest\": RandomForestClassifier(n_estimators=100, random_state=42),\n",
        "    \"Logistic Regression\": LogisticRegression(max_iter=1000, random_state=42),\n",
        "    \"SVC\": SVC(probability=True, random_state=42),\n",
        "    \"Gradient Boosting\": GradientBoostingClassifier(random_state=42),\n",
        "    \"KNN\": KNeighborsClassifier()\n",
        "}\n",
        "\n",
        "\n",
        "def napravi_tabelu(data):\n",
        "    teams = set(data['HomeTeam'].unique()) | set(data['AwayTeam'].unique())\n",
        "    table = pd.DataFrame({'Team': list(teams), 'Points': 0, 'GF': 0, 'GA': 0})\n",
        "\n",
        "    for _, row in data.iterrows():\n",
        "        home = row['HomeTeam']\n",
        "        away = row['AwayTeam']\n",
        "        result = row['Predicted_FTR']\n",
        "        home_goals = row['FTHG']\n",
        "        away_goals = row['FTAG']\n",
        "\n",
        "        table.loc[table['Team'] == home, 'GF'] += home_goals\n",
        "        table.loc[table['Team'] == home, 'GA'] += away_goals\n",
        "        table.loc[table['Team'] == away, 'GF'] += away_goals\n",
        "        table.loc[table['Team'] == away, 'GA'] += home_goals\n",
        "\n",
        "        if result == 'H':\n",
        "            table.loc[table['Team'] == home, 'Points'] += 3\n",
        "        elif result == 'A':\n",
        "            table.loc[table['Team'] == away, 'Points'] += 3\n",
        "        elif result == 'D':\n",
        "            table.loc[table['Team'] == home, 'Points'] += 1\n",
        "            table.loc[table['Team'] == away, 'Points'] += 1\n",
        "\n",
        "    table['GD'] = table['GF'] - table['GA']\n",
        "    table = table.sort_values(by=['Points', 'GD', 'GF'], ascending=False).reset_index(drop=True)\n",
        "    table['Predicted_Position'] = table.index + 1\n",
        "    return table\n",
        "\n",
        "results = []\n",
        "\n",
        "for r in range(6, 9): #Ovdje se bira broj ulaza - sada je postavljeno od 6 do 8\n",
        "    for combo in itertools.combinations(moguci_ulazi, r):\n",
        "        features_subset = list(combo)\n",
        "\n",
        "        X_train = train_data[features_subset]\n",
        "        y_train = train_data[target]\n",
        "        X_eval = eval_data[features_subset]\n",
        "        y_eval = eval_data[target]\n",
        "        X_train_dummies = pd.get_dummies(X_train, drop_first=True)\n",
        "        X_eval_dummies = pd.get_dummies(X_eval, drop_first=True)\n",
        "        X_train_dummies, X_eval_dummies = X_train_dummies.align(X_eval_dummies, join='outer', axis=1, fill_value=0)\n",
        "\n",
        "        for model_name, model in models.items():\n",
        "            model.fit(X_train_dummies, y_train)\n",
        "            y_pred = model.predict(X_eval_dummies)\n",
        "            acc = accuracy_score(y_eval, y_pred)\n",
        "\n",
        "            eval_data_temp = eval_data.copy()\n",
        "            eval_data_temp['Predicted_FTR'] = y_pred\n",
        "\n",
        "            predicted_table = napravi_tabelu(eval_data_temp)\n",
        "            merged_table = predicted_table.merge(actual_table[['Team', 'Actual_Position']], on='Team', how='left')\n",
        "            corr, _ = spearmanr(merged_table['Predicted_Position'], merged_table['Actual_Position'])\n",
        "\n",
        "            results.append({\n",
        "                \"Model\": model_name,\n",
        "                \"Features\": features_subset,\n",
        "                \"Accuracy\": acc,\n",
        "                \"Spearman_Correlation\": corr\n",
        "            })\n",
        "            print(f\"Model: {model_name}, Features: {features_subset}, Accuracy: {acc:.4f}, Spearman: {corr:.4f}\")\n",
        "\n",
        "results_df = pd.DataFrame(results)\n",
        "results_df = results_df.sort_values(by=['Accuracy', 'Spearman_Correlation'], ascending=False).reset_index(drop=True)\n",
        "\n",
        "print(\"\\nFinal Results:\")\n",
        "print(results_df)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "v2svLXvPUX1d",
        "outputId": "b1520c84-d0ef-4222-dddb-fdef4ea060e1"
