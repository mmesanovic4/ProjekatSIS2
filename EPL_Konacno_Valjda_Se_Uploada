        "import pandas as pd\n",
        "\n",
        "df = epl3\n",
        "kolone_za_izbaciti = ['FTR', 'FTHG', 'FTAG']\n",
        "df = df.drop(columns=[col for col in kolone_za_izbaciti if col in df.columns])\n",
        "\n",
        "#Korelacija\n",
        "korelacije = df.corr(numeric_only=True)['FTR_Int'].drop('FTR_Int')\n",
        "\n",
        "najvece = korelacije.abs().nlargest(10)\n",
        "\n",
        "print('10 kolona s najvećom korelacijom:')\n",
        "print(najvece)"
