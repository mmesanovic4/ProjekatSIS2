        "import re\n",
        "\n",
        "accuracy_lines = []\n",
        "\n",
        "with open(\"ZastoJeStao.txt\", \"r\", encoding=\"utf-8\") as file:\n",
        "    for line in file:\n",
        "        match = re.search(r\"Accuracy:\\s*([\\d.]+)\", line)\n",
        "        if match:\n",
        "            accuracy = float(match.group(1))\n",
        "            accuracy_lines.append((accuracy, line.strip()))\n",
        "\n",
        "top_10 = sorted(accuracy_lines, key=lambda x: x[0], reverse=True)[:20]\n",
        "\n",
        "print(\"Top 20 linija prema Accuracy:\")\n",
        "for i, (acc, line) in enumerate(top_10, 1):\n",
        "    print(f\"{i}. Accuracy: {acc} | {line}\")\n"
      ],
