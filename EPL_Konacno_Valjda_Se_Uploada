        "df = epl4\n",
        "\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "#Novi ulazi\n",
        "features = ['HomeTeam', 'AwayTeam', 'Referee','Domacin -1 utakmica',\n",
        "       'Domacin -2 utakmica', 'Domacin -3 utakmica', 'Domacin -4 utakmica',\n",
        "       'Domacin -5 utakmica', 'Gost -1 utakmica', 'Gost -2 utakmica',\n",
        "       'Gost -3 utakmica', 'Gost -4 utakmica', 'Gost -5 utakmica']\n",
        "target = 'FTR'\n",
        "\n",
        "\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "df = epl4\n",
        "\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "#Novi ulazi\n",
        "features = ['HomeTeam', 'AwayTeam', 'Referee', 'Domacin -1 utakmica',\n",
        "       'Domacin -2 utakmica', 'Domacin -3 utakmica', 'Domacin -4 utakmica',\n",
        "       'Domacin -5 utakmica', 'Gost -1 utakmica', 'Gost -2 utakmica',\n",
        "       'Gost -3 utakmica', 'Gost -4 utakmica', 'Gost -5 utakmica']\n",
        "target = 'FTR'\n",
        "\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "\n",
        "print(\"Rezultati za sezonu 2018/19:\")\n",
        "print(\"-\" * 60)\n",
        "eval_data['Date'] = pd.to_datetime(eval_data['Date']).dt.strftime('%d-%m-%Y')\n",
        "for idx, row in eval_data.iterrows():\n",
        "    pred = predictions[idx - eval_data.index[0]]\n",
        "    actual = row[target]\n",
        "    print(f\"Utakmica: {row['HomeTeam']} - {row['AwayTeam']}, Datum: {row['Date']}, \"\n",
        "          f\"Predviđeni rezultat: {pred}, Stvarni rezultat: {actual}\")\n",
        "\n",
        "tacnost = accuracy_score(eval_data[target], predictions)\n",
        "print(\"-\" * 60)\n",
        "print(f\"Ukupna tačnost rezultata: {tacnost:.5f}\")\n"
