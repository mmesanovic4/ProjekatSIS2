    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "# Učitavanje dataset-a (pretpostavljamo da je epl3 već učitan)\n",
        "df = epl5\n",
        "# Filtriranje podataka za sezonu 2018/19\n",
        "eval_data = df[df['Season'] == '2018/2019'].copy()\n",
        "\n",
        "# Konvertovanje kolone 'Date' u datetime, sortiranje po datumu i formatiranje datuma (bez vremena)\n",
        "eval_data['Date'] = pd.to_datetime(eval_data['Date'])\n",
        "eval_data.sort_values(by='Date', inplace=True)\n",
        "eval_data['Date_str'] = eval_data['Date'].dt.strftime('%d-%m-%Y')\n",
        "\n",
        "# Ciljna varijabla\n",
        "target = 'FTR'\n",
        "\n",
        "# Postavljamo veličinu prozora (window) za formu – 5 utakmica\n",
        "window = 5\n",
        "\n",
        "# Inicijalizacija dinamičkih formi – za domaće i gost timove\n",
        "# Ako tim prvi put nastupa, koristimo početnu formu iz odgovarajućih kolona\n",
        "current_form_home = {}\n",
        "current_form_away = {}\n",
        "\n",
        "# Inicijalizacija ligaške tabele: za svaki tim pratimo bodove, pobjede, remije i poraze\n",
        "league_table = {}\n",
        "\n",
        "# Lista u koju spremamo informacije o utakmicama radi ispisa\n",
        "match_info = []\n",
        "\n",
        "# Lista za spremanje predikcija i stvarnih rezultata (radi kasnijeg računa tačnosti)\n",
        "predictions_list = []\n",
        "actual_list = []\n",
        "\n",
        "# Iteriramo kroz utakmice sezone 2018/19 hronološki\n",
        "for pos, (idx, row) in enumerate(eval_data.iterrows()):\n",
        "    home_team = row['HomeTeam']\n",
        "    away_team = row['AwayTeam']\n",
        "\n",
        "    # Ako tim nije već inicijalizovan, koristimo početnu formu iz dataseta\n",
        "    if home_team not in current_form_home:\n",
        "        current_form_home[home_team] = row['Forma Domacina Nova']  # npr. \"WWLLD\"\n",
        "    if away_team not in current_form_away:\n",
        "        current_form_away[away_team] = row['Forma Gosta Nova']\n",
        "\n",
        "    # Kreiramo feature dictionary uključujući dinamički ažurirane forme\n",
        "    feat_dict = {\n",
        "        'HomeTeam': home_team,\n",
        "        'AwayTeam': away_team,\n",
        "        'Referee': row['Referee'],\n",
        "        'Forma Domacina': current_form_home[home_team],\n",
        "        'Forma Gosta': current_form_away[away_team]\n",
        "    }\n",
        "\n",
        "    # Pretvaramo u DataFrame i radimo one-hot enkodiranje\n",
        "    feat_df = pd.DataFrame([feat_dict])\n",
        "    feat_df_encoded = pd.get_dummies(feat_df)\n",
        "    # Uskladite stupce s onima koji su korišteni pri treningu\n",
        "    feat_df_encoded = feat_df_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "    # Predviđanje za trenutnu utakmicu (model je već istreniran)\n",
        "    pred = model.predict(feat_df_encoded)[0]\n",
        "    predictions_list.append(pred)\n",
        "    actual = row[target]\n",
        "    actual_list.append(actual)\n",
        "\n",
        "    # Spremamo informacije o utakmici za ispis\n",
        "    match_info.append({\n",
        "        'Utakmica': f\"{home_team} - {away_team}\",\n",
        "        'Datum': row['Date_str'],\n",
        "        'Forma Domacina': current_form_home[home_team],\n",
        "        'Forma Gosta': current_form_away[away_team],\n",
        "        'Predvidjeni rezultat': pred,\n",
        "        'Stvarni rezultat': actual\n",
        "    })\n",
        "\n",
        "    # Inicijalizacija timova u ligaškoj tabeli ako već nisu prisutni\n",
        "    for team in [home_team, away_team]:\n",
        "        if team not in league_table:\n",
        "            league_table[team] = {'Bodovi': 0, 'Pobjede': 0, 'Remiji': 0, 'Porazi': 0}\n",
        "\n",
        "    # Ažuriramo ligašku tabelu na osnovu predikcije:\n",
        "    # - Ako je predikcija 'H': domaćin dobiva 3 boda, gost 0; 'A': obrnuto; 'D': oboje 1 bod.\n",
        "    if pred == 'H':\n",
        "        league_table[home_team]['Bodovi'] += 3\n",
        "        league_table[home_team]['Pobjede'] += 1\n",
        "        league_table[away_team]['Porazi'] += 1\n",
        "        new_home_outcome = 'W'\n",
        "        new_away_outcome = 'L'\n",
        "    elif pred == 'A':\n",
        "        league_table[away_team]['Bodovi'] += 3\n",
        "        league_table[away_team]['Pobjede'] += 1\n",
        "        league_table[home_team]['Porazi'] += 1\n",
        "        new_home_outcome = 'L'\n",
        "        new_away_outcome = 'W'\n",
        "    else:  # Remi ('D')\n",
        "        league_table[home_team]['Bodovi'] += 1\n",
        "        league_table[away_team]['Bodovi'] += 1\n",
        "        league_table[home_team]['Remiji'] += 1\n",
        "        league_table[away_team]['Remiji'] += 1\n",
        "        new_home_outcome = 'D'\n",
        "        new_away_outcome = 'D'\n",
        "\n",
        "    # Ažuriranje dinamičke forme za domaćina\n",
        "    if len(current_form_home[home_team]) >= window:\n",
        "        current_form_home[home_team] = current_form_home[home_team][1:] + new_home_outcome\n",
        "    else:\n",
        "        current_form_home[home_team] += new_home_outcome\n",
        "\n",
        "    # Ažuriranje dinamičke forme za gosta\n",
        "    if len(current_form_away[away_team]) >= window:\n",
        "        current_form_away[away_team] = current_form_away[away_team][1:] + new_away_outcome\n",
        "    else:\n",
        "        current_form_away[away_team] += new_away_outcome\n",
        "\n",
        "# Izračun ukupne tačnosti predikcija\n",
        "tacnost = accuracy_score(actual_list, predictions_list)\n",
        "\n",
        "# Ispis rezultata utakmica\n",
        "print(\"Rezultati za sezonu 2018/19:\")\n",
        "print(\"-\" * 60)\n",
        "for m in match_info:\n",
        "    print(f\"Utakmica: {m['Utakmica']}, Datum: {m['Datum']}, \"\n",
        "          f\"Forma Domacina: {m['Forma Domacina']}, Forma Gosta: {m['Forma Gosta']}, \"\n",
        "          f\"Predvidjeni rezultat: {m['Predvidjeni rezultat']}, Stvarni rezultat: {m['Stvarni rezultat']}\")\n",
        "\n",
        "print(\"-\" * 60)\n",
        "print(f\"Ukupna tačnost rezultata: {tacnost:.5f}\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
