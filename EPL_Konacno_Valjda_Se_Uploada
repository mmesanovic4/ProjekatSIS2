      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "# Učitavanje dataset-a\n",
        "df = epl4\n",
        "\n",
        "# Filtriranje podataka za sezonu 2018/19\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "# Odabir relevantnih kolona\n",
        "features = ['HomeTeam', 'AwayTeam','HTR', 'Poredjenje_Suteva', 'Poredjenje_Suteva_u_Okvir', 'Poredjenje_Crvenih_Kartona', 'Poredjenje_Zutih_Kartona']\n",
        "target = 'FTR'  # pretpostavljamo da je FTR ciljna varijabla (npr. H/D/A)\n",
        "\n",
        "# Priprema podataka za evaluaciju\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "\n",
        "# Uskladite stupce s onima koji su korišteni pri treningu\n",
        "# (pretpostavljamo da ste već kreirali X_train_encoded iz skupa podataka prije 2017/18)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "# Predviđanje (pretpostavljamo da je model već istreniran)\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "\n",
        "import pandas as pd\n",
        "from sklearn.metrics import accuracy_score\n",
        "\n",
        "# Učitavanje dataset-a\n",
        "df = epl4\n",
        "\n",
        "# Filtriranje podataka za sezonu 2018/19\n",
        "eval_data = df[df['Season'] == '2018/2019']\n",
        "\n",
        "# Odabir relevantnih kolona\n",
        "features = ['HomeTeam', 'AwayTeam','HTR', 'Poredjenje_Suteva', 'Poredjenje_Suteva_u_Okvir', 'Poredjenje_Crvenih_Kartona', 'Poredjenje_Zutih_Kartona']\n",
        "target = 'FTR'  # pretpostavljamo da je FTR ciljna varijabla (npr. H/D/A)\n",
        "\n",
        "# Priprema podataka za evaluaciju\n",
        "X_eval = eval_data[features]\n",
        "X_eval_encoded = pd.get_dummies(X_eval)\n",
        "\n",
        "# Uskladite stupce s onima koji su korišteni pri treningu\n",
        "# (pretpostavljamo da ste već kreirali X_train_encoded iz skupa podataka prije 2017/18)\n",
        "X_eval_encoded = X_eval_encoded.reindex(columns=X_train_encoded.columns, fill_value=0)\n",
        "\n",
        "# Predviđanje (pretpostavljamo da je model već istreniran)\n",
        "predictions = model.predict(X_eval_encoded)\n",
        "\n",
        "# Ispis rezultata: Utakmica: timA - timB, datum, predviđeni rezultat, stvarni rezultat\n",
        "print(\"Rezultati za sezonu 2018/19:\")\n",
        "print(\"-\" * 60)\n",
        "# Konverzija kolone 'Date' u format bez vremena\n",
        "eval_data['Date'] = pd.to_datetime(eval_data['Date']).dt.strftime('%d-%m-%Y')\n",
        "# Iteriramo kroz redove evaluacijskog skupa i ispisujemo rezultate\n",
        "for idx, row in eval_data.iterrows():\n",
        "    # Povezujemo red sa pripadajućom predikcijom (pretpostavljamo da indeksi odgovaraju)\n",
        "    pred = predictions[idx - eval_data.index[0]]  # ako indeksi nisu 0-indeksirani\n",
        "    actual = row[target]\n",
        "    print(f\"Utakmica: {row['HomeTeam']} - {row['AwayTeam']}, Datum: {row['Date']}, \"\n",
        "          f\"Predviđeni rezultat: {pred}, Stvarni rezultat: {actual}\")\n",
        "\n",
        "# Izračun ukupne tačnosti\n",
        "tacnost = accuracy_score(eval_data[target], predictions)\n",
        "print(\"-\" * 60)\n",
        "print(f\"Ukupna tačnost rezultata: {tacnost:.5f}\")\n"
      ],
