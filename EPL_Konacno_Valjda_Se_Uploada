        "\n",
        "actual_table = pd.DataFrame(stvarna_tabela)\n",
        "actual_table.rename(columns={\"Position\": \"Actual_Position\"}, inplace=True)\n",
        "\n",
        "print(actual_table)\n"
      ],
      "metadata": {
